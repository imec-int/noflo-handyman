{ "name": "noflo-handyman"
, "description": "Handy tools for debugging in NoFlo"
, "keywords":
  [ "noflo"
  , "debugging"
  , "tools"
  ]
, "author": "Kenneth Kan <kenhkan@gmail.com>"
, "version": "0.0.1"
, "licenses":
  [ { "type": "MIT"
    , "url": "https://github.com/kenhkan/noflo-handyman/blob/master/LICENSE.md"
    }
  ]
, "engines":
  { "node": ">=0.6.0"
  }
, "repository":
  { "type": "git"
  , "url": "git://github.com/kenhkan/noflo-handyman"
  }
, "dependencies":
